.title_p {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  box-sizing: border-box;
  display: flex;
  align-items: centers;
  position: relative;
  z-index: 2;
}

.title_p .van-icon {
  color: rgba(51, 51, 51, 1);
  margin-left: 6rpx;
}

.flex_box .value_p {
  margin-left: 12rpx;
}

.flex_box .title_p {
  margin-left: 12rpx;
}

.flex_box .detail_flex_box .value_p {
  margin-left: 0;
}

.flex_box .detail_flex_box .title_p {
  margin-left: 0;
  text-align: center;
}

.value_p {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  box-sizing: border-box;
  position: relative;
  z-index: 2;
}

.detail_flex_box {
  box-sizing: border-box;
  height: 136rpx;
  background: rgba(239, 239, 239, 1);
  border-radius: 6px;
  display: flex;
  justify-content: space-around;
  padding: 20rpx 0;
}

.detail_flex_box .cell_box {
  box-sizing: border-box;
  width: calc(100%/3);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.zhuanyuan_container .detail_flex_box .cell_box {
  width: calc(100%/2);
}

.yunyingshang_container .detail_flex_box .cell_box {
  width: calc(100%/2);
}

.border {
  border-right: 2rpx solid rgba(0, 0, 0, 0.1);
}

.select_box {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  padding: 24rpx;
  background-color: #fff;
  margin: 24rpx 0 0 0;
}

.select_box .p_desc {
  display: inline-flex;
  align-items: center;
  margin-right: 74rpx;
}

.select_box .p_desc .van-icon {
  margin-left: 14rpx;
}

.statisticsPanel {
  box-sizing: border-box;
  padding: 0 24rpx 24rpx 24rpx;
}

.update_time {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-top: 24rpx;
  box-sizing: border-box;
  padding: 0 24rpx;
  text-align: right;
}

