/* pages/home/home.wxss */

page {
  background: #ededed;
}

.home {
  width: 100%;
  height: 448rpx;
  background: linear-gradient(180deg, rgba(14, 134, 254, 1) 0%, rgba(14, 134, 254, 1) 39%, rgba(0, 105, 255, 1) 100%);
}

.home0 {
  height: 336rpx !important;
}

.home1 {
  height: 336rpx !important;
}

.home2 {
  height: 386rpx !important;
}

/*头部样式*/

.nav {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}

/*搜索样式*/

.nav-title {
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-size: 40rpx;
  letter-spacing: 2px;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.bg {
  position: absolute;
  z-index: 5;
  left: 0;
  right: 0;
  width: 100%;
  min-height: 140rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.container {
  box-sizing: border-box;
  padding: 24rpx 24rpx 24rpx 24rpx;
  margin: 24rpx 24rpx 0 24rpx;
  /* height: 488rpx; */
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.1);
  border-radius: 10px;
  position: relative;
  z-index: 10;
}

.flex_box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.doctor_num {
  width: 316rpx;
  height: 136rpx;
  position: relative;
  z-index: 2;
  margin-bottom: 24rpx;
  padding: 20rpx 6rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-radius: 6px;
  background: url('https://pfks-oss-center.oss-cn-beijing.aliyuncs.com/icon_doctor_num.png') no-repeat center;
  background-size: cover;
}

