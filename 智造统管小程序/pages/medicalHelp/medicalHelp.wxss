/* 下拉动画 */
/* @keyframes Css3 新标签，循环动画 */
@keyframes slidown{
   0%{
        transform:  translateY(-100%);
        opacity: 0;
    }
    100%{
        transform:  translateY(0%);
        opacity: 1;
    }
}
.slidown{
    animation: slidown  2s  ;
    animation-fill-mode:forwards;
    /* display: none; */
}
 
/* 上拉动画 */
@keyframes slidup
	{
	0%{
        transform:  translateY(0%);
        opacity: 1;
    }
    50%{
        transform:  translateY(-100%);
        opacity: 0;
    }
    100%{
        height: 30rpx;
        opacity: 0;
    }
}
.slidup{
  /* display: block; */
    animation: slidup  4s;
    animation-fill-mode:forwards;
}
