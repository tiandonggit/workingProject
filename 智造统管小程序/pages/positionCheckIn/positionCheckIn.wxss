Page {
  background-color: #f4f4f4;
}

.wholeContinaer {
  position: relative;
  height: 100vh;
}

.mapContianer {
  background-color: turquoise;
  width: 100vw;
  height: 100vh;
}

.checkInPanel {
  position: absolute;
  height: 498rpx;
  bottom: 0;
  left: 0;
  right: 0;
}

.checkInBtn {
  width: 218rpx;
  height: 218rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  position: absolute;
  left: calc(50% - 109rpx);
  z-index: 12;
  border-radius: 50%;
  background-color: #00a0e9;
  /* box-shadow: 0 0 10rpx 0rpx #0089e9; *//* margin: 0 auto; */
}

.withOutPermission {
  background-image: none !important;
  background-color: rgba(0, 0, 0, 0.4);
}

.disableClick {
  pointer-events: none;
}

.checkInTitle {
  font-size: 40rpx;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.checkInTime {
  font-size: 28rpx;
  line-height: 34rpx;
  color: rgba(255, 255, 255, 1);
  margin-top: 12rpx;
}

.whitePositionPanel {
  width: 100%;
  height: 396rpx;
  position: absolute;
  bottom: 0rpx;
  background-color: #fff;
  border-radius: 10rpx;
  /* display: flex;
  align-items: center;
  justify-content: center; */
}

.positonTextRow {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 130rpx;
  margin-bottom: 30rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.positonTextRow .positionIcon {
  display: block;
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.positonTextRow .positionFont {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.border {
  width: 100%;
  height: 20rpx;
  background: rgba(237, 237, 237, 1);
}

.daka_box {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 30rpx;
}

.daka_box .title {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.divider {
  width: 100%;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0 72rpx;
  margin-top: 16rpx;
  justify-content: space-around;
}

.divider .rpxBorder {
  width: 200rpx;
  height: 2rpx;
  background-color: rgba(0, 0, 0, 0.1);
}

.divider .divider_content {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
}

.divider .divider_content .van-icon {
  color: rgba(153, 153, 153, 1);
  font-size: 36rpx;
}

.noAddressName {
  color: rgba(153, 153, 153, 1) !important;
}

.van-popup--bottom {
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px -2px 8px 0px rgba(0, 0, 0, 0.3);
  border-radius: 10px 10px 0px 0px;
  padding: 30rpx 24rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.van-popup--bottom .scroll_box {
  flex: 1;
  height: 0;
  box-sizing: border-box;
  padding: 12rpx 0 24rpx 0;
}

.historyList_title_box {
  display: flex;
  flex-direction: column;
}

.historyList_title_box .history_title {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

.item_view {
  margin-bottom: 10rpx;
}

.item_box {
  margin-bottom: 20rpx;
}

.flex {
  display: flex;
  align-items: center;
}

.span {
  display: inline-block;
  width: 100%;
}

.flex .positionIcon {
  display: block;
  width: 32rpx;
  height: 32rpx;
  margin-right: 6rpx;
}

.mgl_box {
  margin-left: 38rpx;
}

.address {
  font-size: 32rpx;
  line-height: 40rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.detail_address {
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.location_agin {
  position: absolute;
  z-index: 100;
  right: 24rpx;
  bottom: 408rpx;
  width: 152rpx;
  height: 56rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 14px;
  border: 1px solid rgba(0, 105, 255, 1);
  text-align: center;
  line-height: 56rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(0, 105, 255, 1);
}

.van-dialog__message {
  font-size: 32rpx !important;
  line-height: 40rpx !important;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  padding: 60rpx;
  text-align: center;
}

.van-dialog__footer {
  padding-top: 0;
  padding-bottom: 60rpx;
}

.vant-button {
  height: auto !important;
}

.van-button--large {
  height: auto !important;
}

.van-hairline--top:after {
  border-top-width: 0 !important;
}

.van-button__text {
  width: 276rpx !important;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  background: linear-gradient(180deg, rgba(0, 162, 255, 1) 0%, rgba(0, 105, 255, 1) 100%);
  border-radius: 4px;
  font-size: 32rpx !important;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.dialog-index--van-dialog {
  width: 542rpx !important;
}

.icon_bottom {
  display: block;
  width: 40rpx;
  height: 40rpx;
}

.icon_top {
  display: block;
  width: 40rpx;
  height: 40rpx;
}
