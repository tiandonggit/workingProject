/* pages/home/home.wxss */

page {
  background: #ededed;
}

.statisticalHome {
  width: 100%;
  height: 330rpx;
}

/*头部样式*/

.nav {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}

.nav-title {
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-size: 36rpx;
  letter-spacing: 2px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.code {
  width: 66rpx;
  height: 100%;
  float: left;
  position: relative;
}

.code_pic {
  width: 36rpx;
  height: 35rpx;
  float: left;
  position: absolute;
  top: 20rpx;
  left: 10rpx;
}

.back_pic {
  width: 50rpx;
  height: 50rpx;
  float: left;
  position: absolute;
  top: 20rpx;
  left: 10rpx;
}

.header {
  width: 100%;
  /* height: 118rpx; */
  background: #fff;
}

.cont_body {
  font-size: 32rpx;
  padding: 30rpx 24rpx;
}

.cont_body .top-info {
  font-size: 40rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
  margin-bottom: 20rpx;
}

.cont_body .top-info text {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.cont_body .tab-choose {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cont_body .tab-choose .tab0 {
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
  text-align: center;
}

.cont_body .tab-choose .tab1 {
  width: 124rpx;
  height: 40rpx;
  background: #64c4fd;
  border-radius: 8rpx;
  color: rgba(255, 255, 255, 1);
}

.cont_body .tab-choose .tab3 {
  width: 152rpx;
  height: 40rpx;
  background: linear-gradient(45deg, rgba(42, 230, 126, 1) 0%, rgba(2, 192, 89, 1) 100%);
  border-radius: 0rpx 22rpx 22rpx 20rpx;
  color: rgba(255, 255, 255, 1);
}

.cont_body .tab-choose .tab4 {
  width: 128rpx;
  height: 40rpx;
  background: linear-gradient(45deg, rgba(0, 162, 255, 1) 0%, rgba(0, 105, 255, 1) 100%);
  border-radius: 0rpx 22rpx 22rpx 20rpx;
  color: rgba(255, 255, 255, 1);
}

.cont_body .tab-choose .tab5 {
  width: 98rpx;
  height: 40rpx;
  background: linear-gradient(45deg, rgba(255, 146, 0, 1) 0%, rgba(255, 89, 0, 1) 100%);
  border-radius: 0rpx 22rpx 22rpx 20rpx;
  color: rgba(255, 255, 255, 1);
}

.container {
  box-sizing: border-box;
  padding: 24rpx 24rpx 24rpx 24rpx;
  margin: 24rpx 24rpx 0 24rpx;
  /* height: 488rpx; */
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.1);
  border-radius: 10px;
  position: relative;
  z-index: 10;
}

.flex_box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.doctor_num {
  width: 316rpx;
  height: 136rpx;
  position: relative;
  z-index: 2;
  margin-bottom: 24rpx;
  padding: 20rpx 6rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-radius: 6px;
  background: url('https://pfks-oss-center.oss-cn-beijing.aliyuncs.com/icon_doctor_num.png') no-repeat center;
  background-size: cover;
}

.patient_num {
  width: 316rpx;
  height: 136rpx;
  position: relative;
  z-index: 2;
  margin-bottom: 24rpx;
  padding: 20rpx 12rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-radius: 6px;
  background: url('https://pfks-oss-center.oss-cn-beijing.aliyuncs.com/icon_patient_num.png') no-repeat center;
  background-size: cover;
}

.hospital_num {
  width: 316rpx;
  height: 136rpx;
  position: relative;
  z-index: 2;
  margin-bottom: 24rpx;
  padding: 20rpx 12rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-radius: 6px;
  background: url('https://pfks-oss-center.oss-cn-beijing.aliyuncs.com/icon_hospital_num.png') no-repeat center;
  background-size: cover;
}

.gui_number {
  width: 316rpx;
  height: 136rpx;
  position: relative;
  z-index: 2;
  margin-bottom: 24rpx;
  padding: 20rpx 12rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-radius: 6px;
  background: url('https://pfks-oss-center.oss-cn-beijing.aliyuncs.com/icon_gui_num.png') no-repeat center;
  background-size: cover;
}

.title_p {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  box-sizing: border-box;
  display: flex;
  align-items: centers;
}

.title_p .van-icon {
  color: rgba(51, 51, 51, 1);
  margin-left: 6rpx;
}

.flex_box .value_p {
  margin-left: 12rpx;
}

.flex_box .title_p {
  margin-left: 12rpx;
}

.flex_box .detail_flex_box .value_p {
  margin-left: 0;
}

.flex_box .detail_flex_box .title_p {
  margin-left: 0;
  text-align: center;
}

.value_p {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  box-sizing: border-box;
}

.detail_flex_box {
  box-sizing: border-box;
  height: 136rpx;
  background: rgba(239, 239, 239, 1);
  border-radius: 6px;
  display: flex;
  justify-content: space-around;
  padding: 20rpx 0;
}

.detail_flex_box .cell_box {
  box-sizing: border-box;
  width: calc(100% / 3);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.zhuanyuan_container .detail_flex_box .cell_box {
  width: calc(100% / 2);
}

.yunyingshang_container .detail_flex_box .cell_box {
  width: calc(100% / 2);
}

.border {
  border-right: 2rpx solid rgba(0, 0, 0, 0.1);
}

.select_box {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  padding: 24rpx;
  background-color: #fff;
  margin: 24rpx 0 0 0;
}

.select_box .p_desc {
  display: inline-flex;
  align-items: center;
  margin-right: 74rpx;
}

.select_box .p_desc .van-icon {
  margin-left: 14rpx;
}

.statisticsPanel {
  box-sizing: border-box;
  padding: 0 24rpx 24rpx 24rpx;
}

.update_time {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-top: 24rpx;
  box-sizing: border-box;
  padding: 0 24rpx;
  text-align: right;
}

/* 选择日期弹出层 */

.popup van-popup .van-popup {
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  width: 412rpx;
}

.popup van-popup .van-popup .chooseTime van-radio-group van-radio .van-radio {
  margin: 0 20rpx;
  padding: 30rpx 0;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
}

.popup van-popup .van-popup .chooseTime van-radio-group van-radio:last-child .van-radio {
  border: 0;
}

.popup van-popup .van-popup .chooseTime van-radio-group van-radio .van-radio .van-radio__icon-wrap {
  font-size: 20rpx;
}

.popup van-popup .van-popup .chooseTime van-radio-group van-radio .van-radio .van-radio__label {
}

.tab {
  width: 128rpx;
  height: 60rpx;
  padding: 0 20rpx;
  margin-left: 20rpx;
  margin-bottom: 30rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  text-align: center;
  color: #333;
  background: #ededed;
