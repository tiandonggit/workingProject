/* components/labels/labels.wxss */
.main {}
.label {
  width: 460rpx;
  display: flex;
  border:1px solid #0069ff;
  border-radius:10px;
  padding: 0 30rpx;
}
.label view {
  width: 230rpx;
  line-height: 56rpx;
  text-align: center;
  color: #0069FF;
  z-index: 5;
}
.label .select {
  color: #FFF;
}
.label .bottom_border {
  position: absolute;
  left: -30rpx;
  z-index: 1;
  height: 56rpx;
  background:linear-gradient(225deg,rgba(1,128,255,1) 0%,rgba(0,105,255,1) 100%);
  border-radius:8px;
}
