/* auditIndex.wxss */
.main {
  position: fixed;
  width: 100%;
  top: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.main_no_scroll {
  width: 100%
}
.label {
  padding: 30rpx 24rpx 0;

}
.search .van-search {
  padding: 30rpx 24rpx;
}
.search .van-search__content {
  border-radius: 12rpx;
}
.main_scroll {
  flex-grow:1;
  overflow: hidden;
  /* padding:0 24rpx; */
  -webkit-overflow-scrolling: touch;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);  
}