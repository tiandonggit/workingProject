/* auditIndex.wxss */
.main {
  position: fixed;
  width: 100%;
  top: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}

.bg {
  flex-grow:1;
  overflow: hidden;
  /* padding:0 24rpx; */
  -webkit-overflow-scrolling: touch;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom); 
  text-align: center;
}

.bg image {
  width: 208rpx;
  position: absolute;
  bottom: 120rpx;
  left: 50%;
  margin-left: -104rpx;
}